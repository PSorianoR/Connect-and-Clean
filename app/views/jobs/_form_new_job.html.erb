<%= simple_form_for [@property, @job] do |f| %>
  <%= f.association :property %>
  <%= f.input :price, input_html: { value: @property.default_job_price } %>
  <%= f.input :description %>
  <%= f.input :date_of_job %>
  <%= f.input :cleaning_from, input_html: { value: @property.default_cleaning_from }  %>
  <%= f.input :cleaning_until, input_html: { value: @property.default_cleaning_until }  %>
  <%# <%= f.collection_check_boxes :options, [[true, 'My Cleaners'], [false, 'Everyone']], :first, :last %>
  <%= f.submit %>
<% end %>
